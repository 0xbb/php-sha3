{
    "name": "bb/php-sha3",
    "description": "Pure PHP implementation of SHA-3",
    "keywords": ["sha3", "sha-3", "shake"],
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "satooshi/php-coveralls": "dev-master"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Bierbaumer",
            "homepage": "https://github.com/0xbb/php-sha3"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "autoload": {
        "psr-4": {
            "bb\\Sha3\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bb\\Sha3\\Test\\": "test"
        }
    }
}
